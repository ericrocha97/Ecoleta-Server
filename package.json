    "knex": "^0.21.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^17.1.2",
    "@types/multer": "^1.4.3",
    "ts-node": "^8.10.2",
